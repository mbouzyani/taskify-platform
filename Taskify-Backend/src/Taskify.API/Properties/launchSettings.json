{
  "$schema": "https://json.schemastore.org/launchsettings.json",
  "profiles": {
    "http": {
      "commandName": "Project",
      "dotnetRunMessages": true,
      "launchBrowser": false,
      "applicationUrl": "http://localhost:5217",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      }
    },
    "https": {
      "commandName": "Project",
      "dotnetRunMessages": true,
      "launchBrowser": false,
      "applicationUrl": "https://localhost:7172;http://localhost:5217",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      }
    },
    "Taskify.API": {
      "commandName": "Project",
      "dotnetRunMessages": true,
      "launchBrowser": true,
      "launchUrl": "swagger",
      "applicationUrl": "https://localhost:7172;http://localhost:5217",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      }
    },
    "Visual Studio Enterprise": {
      "commandName": "Project",
      "dotnetRunMessages": true,
      "launchBrowser": true,
      "launchUrl": "swagger",
      "applicationUrl": "https://localhost:7172;http://localhost:5000",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development",
        "ASPNETCORE_DETAILED_ERRORS": "true",
        "ASPNETCORE_LOGGING__CONSOLE__DISABLECOLORS": "false",
        "ASPNETCORE_LOGGING__LOGLEVEL__DEFAULT": "Information",
        "ASPNETCORE_LOGGING__LOGLEVEL__MICROSOFT.ASPNETCORE": "Warning"
      }
    },
    "Docker": {
      "commandName": "Docker",
      "launchBrowser": true,
      "launchUrl": "{Scheme}://{ServiceHost}:{ServicePort}/swagger",
      "environmentVariables": {
        "ASPNETCORE_URLS": "http://+:8080"
      },
      "publishAllPorts": true,
      "useSSL": false
    },
    "Docker Compose": {
      "commandName": "DockerCompose",
      "launchBrowser": true,
      "launchUrl": "http://localhost:5217/swagger",
      "environmentVariables": {},
      "composeLaunchServiceName": "taskify-api",
      "composeLaunchAction": "LaunchBrowser"
    },
    "Docker Debug": {
      "commandName": "DockerCompose",
      "launchBrowser": true,
      "launchUrl": "http://localhost:5217/swagger",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      },
      "composeLaunchServiceName": "taskify-api-debug",
      "composeLaunchAction": "LaunchBrowser"
    }
  },
  "iisSettings": {
    "windowsAuthentication": false,
    "anonymousAuthentication": true,
    "iisExpress": {
      "applicationUrl": "http://localhost:24893",
      "sslPort": 44383
    }
  }
}
